{
  "name": "ehr-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "bun@1.1.0",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "dev": "vite",
    "dev:web": "vite",
    "build": "tsc && vite build --mode desktop",
    "build:web": "tsc && vite build",
    "build:desktop": "tsc && vite build --mode desktop",
    "lint": "biome check . && node scripts/check-file-sizes.js",
    "lint:fix": "biome check --write .",
    "lint:fix:unsafe": "biome check --write --unsafe .",
    "format": "biome format --write .",
    "check:size": "node scripts/check-file-sizes.js",
    "preview": "vite preview",
    "preview:web": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "prepare": "husky",
    "commit": "git-cz",
    "release": "standard-version --config .versionrc.json",
    "release:minor": "standard-version --release-as minor --config .versionrc.json",
    "release:major": "standard-version --release-as major --config .versionrc.json",
    "release:patch": "standard-version --release-as patch --config .versionrc.json"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-router": "^1.139.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "immer": "^11.0.1",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/router-devtools": "^1.139.10",
    "@tanstack/router-plugin": "^1.139.10",
    "@tanstack/router-vite-plugin": "^1.139.10",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/cli": "^2.9.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "glob": "^11.0.0"
  }
}
