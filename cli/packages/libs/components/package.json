{
  "name": "@health-v1/ui-components",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./button": {
      "types": "./dist/components/button.d.ts",
      "default": "./dist/components/button.js"
    },
    "./input": {
      "types": "./dist/components/input.d.ts",
      "default": "./dist/components/input.js"
    },
    "./label": {
      "types": "./dist/components/label.d.ts",
      "default": "./dist/components/label.js"
    },
    "./box": {
      "types": "./dist/components/box.d.ts",
      "default": "./dist/components/box.js"
    },
    "./flex": {
      "types": "./dist/components/flex.d.ts",
      "default": "./dist/components/flex.js"
    },
    "./stack": {
      "types": "./dist/components/stack.d.ts",
      "default": "./dist/components/stack.js"
    },
    "./card": {
      "types": "./dist/components/card.d.ts",
      "default": "./dist/components/card.js"
    },
    "./avatar": {
      "types": "./dist/components/avatar.d.ts",
      "default": "./dist/components/avatar.js"
    },
    "./dropdown-menu": {
      "types": "./dist/components/dropdown-menu.d.ts",
      "default": "./dist/components/dropdown-menu.js"
    },
    "./badge": {
      "types": "./dist/components/badge.d.ts",
      "default": "./dist/components/badge.js"
    },
    "./container": {
      "types": "./dist/components/container.d.ts",
      "default": "./dist/components/container.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@health-v1/shared": "workspace:*",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
