{
  "name": "lazarus-life-hims",
  "version": "1.4.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:desktop": "tsc --noEmit && vite build --mode desktop && tauri build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "check:size": "node scripts/check-file-sizes.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:mobile": "playwright test --grep @mobile",
    "test:e2e:api": "playwright test --grep @api",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@cubejs-client/core": "^1.6.7",
    "@cubejs-client/react": "^1.6.7",
    "@hookform/resolvers": "^5.2.2",
    "@lazarus-life/shared": "workspace:*",
    "@lazarus-life/ui-components": "workspace:*",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/rapier": "^2.2.0",
    "@tauri-apps/api": "^2.5.0",
    "@types/three": "^0.182.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.564.0",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "three": "^0.182.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.5.0",
    "@axe-core/react": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@tanstack/router-plugin": "^1.141.2",
    "@tanstack/router-vite-plugin": "^1.141.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "axe-core": "^4.11.0",
    "glob": "^13.0.0",
    "jsdom": "^27.3.0",
    "vitest": "^4.0.15"
  }
